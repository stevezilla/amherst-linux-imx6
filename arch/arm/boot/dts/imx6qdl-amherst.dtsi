&ecspi2 {
	fsl,spi-num-chipselects = <1>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi2 &pinctrl_ecspi2_cs_1>;
	status = "okay";

	spidev0:spidev@0 {
		compatible = "spidev";
		spi-max-frequency = <57600000>;
		reg = <0>;
	};
        
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet_1>;
	phy-mode = "rmii";
	phy-reset-gpios = <&gpio1 3 0>;
	status = "okay";
};

/* MOE: check clocks */
&i2c1 {
	status = "okay";
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1_2>;

	codec: sgtl5000@0a {
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		clocks = <&clks 201>;
		/*clock-names = "csi_mclk";
		mclk = <24000000>;
		mclk_source = <0>;*/
		VDDA-supply = <&reg_3p3v>;
		VDDIO-supply = <&reg_3p3v>;
		/*DOVDD-supply = <&reg_3p3v>;
		AVDD-supply = <&reg_3p3v>;
		DVDD-supply = <&reg_3p3v>;*/
	};
};

&i2c2 {
	status = "okay";
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2_1>;

	hdmi: edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};
};

/* MOE: Check ssi1 */
&ssi1 {
	fsl,mode = "i2s-slave";
	pinctrl-0 = <&pinctrl_audmux_2>;
	status = "okay";
};

&uart1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1_1>;
};

&uart2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart2_3>;
};

&uart4 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart4_2>;
};

/* WiFi setup */
&ssi3 {
	fsl,mode = "i2s-slave";
	pinctrl-0 = <&pinctrl_audmux_5>;
	status = "okay";
};
&uart5 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart5_1>;
};

&i2c3 {
	status = "okay";
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3_1>;

	cmeh: cmeh@20 {
		compatible = "fsl,imx6-cmeh-i2c";
		reg = <0x20>;
	};
};

&usdhc3 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_wifi>;
	vmmc-supply = <&reg_3p3v>;
	no-1-8-v;
	bus-width = <4>;
	status = "okay";
	wifi-host;
	non-removable;
	keep-power-in-suspend;
};
/* End of WiFi setup */

&usbh1 {
	status = "okay";
};

&usbotg {
	vbus-supply = <&reg_usb_otg_vbus>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usbotg_1>;
	disable-over-current;
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2_1>;
	vmmc-supply = <&reg_3p3v>;
	no-1-8-v;
	bus-width = <4>;
	status = "okay";
};

&usdhc4 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc4_1>;
	vmmc-supply = <&reg_3p3v>;
	no-1-8-v;
	bus-width = <8>;
	status = "okay";
};

&flexcan2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan2_1>;
	status = "okay";
};


/* MOE: Check iomuxc */
&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;

	hog {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				/* SD card power enable */
				MX6QDL_PAD_GPIO_18__GPIO7_IO13		0x80000000
				MX6QDL_PAD_DISP0_DAT22__GPIO5_IO16		0x80000000
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17		0x80000000
				/* Clock for WiFi and SGTL5000 */
				MX6QDL_PAD_GPIO_0__CCM_CLKO1    	0x130b0
			>;
		};
	};
};

